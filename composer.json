{
    "name": "thejoshsmith/craft-commerce-multi-vendor",
    "description": "Support multi vendors on your Craft Commerce store.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft commerce multi vendor"
    ],
    "support": {
        "docs": "https://github.com/thejoshsmith/craft-commerce-multi-vendor/blob/master/README.md",
        "issues": "https://github.com/thejoshsmith/craft-commerce-multi-vendor/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Smith",
            "homepage": "https://www.joshsmith.dev"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "thejoshsmith\\craftcommercemultivendor\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Commerce Multi Vendor",
        "handle": "craft-commerce-multi-vendor",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/thejoshsmith/craft-commerce-multi-vendor/master/CHANGELOG.md",
        "components": {
            "vendors": "thejoshsmith\\craftcommercemultivendor\\services\\Vendors",
            "purchases": "thejoshsmith\\craftcommercemultivendor\\services\\Purchases"
        },
        "class": "thejoshsmith\\craftcommercemultivendor\\Plugin"
    }
}
